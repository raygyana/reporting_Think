{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "insight-think"
  },
  "apps": [{
      "name": "mit",
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "mps.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/font-awesome/css/font-awesome.min.css",
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "./assets/css/dataTables.bootstrap4.min.css",
        "../node_modules/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css",
        "../node_modules/datatables.net-buttons-dt/css/buttons.dataTables.css",
        "../node_modules/datatables.net-colreorder-dt/css/colReorder.dataTables.css",
        "../node_modules/datatables.net-select-dt/css/select.dataTables.css",
        "styles.css",
        "./assets/css/mps.min.css",
        "./assets/css/main.css",
        "./assets/css/icon-style.css"
      ],
      "scripts": [


        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/tether/dist/js/tether.min.js",
        "../node_modules/popper.js/dist/umd/popper.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.min.js",
        "../node_modules/datatables.net/js/jquery.dataTables.js",
        "./assets/js/dataTables.bootstrap4.min.js",
        "../node_modules/datatables.net-buttons/js/dataTables.buttons.js",
        "../node_modules/datatables.net-buttons/js/buttons.colVis.js",
        "../node_modules/datatables.net-buttons/js/buttons.flash.js", "../node_modules/jszip/dist/jszip.min.js",
        "../node_modules/datatables.net-buttons/js/buttons.html5.js",
        "../node_modules/datatables.net-buttons/js/buttons.print.js",
        "../node_modules/datatables.net-colreorder/js/dataTables.colReorder.js",
        "../node_modules/datatables.net-select/js/dataTables.select.js",
        "../node_modules/pdfmake/build/pdfmake.min.js",
        "../node_modules/pdfmake/build/vfs_fonts.js",
        "../node_modules/moment/moment.js",
        "./assets/HighChart/highcharts.js",
        "./assets/HighChart/data.js",
        "./assets/HighChart/drilldown.js",
        "./assets/HighChart/sunburst.js",
        "./assets/jQuery-slimScroll-1.3.8/jquery.slimscroll.min.js",
        "../node_modules/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js",
        "./assets/js/d3.v3.min.js",
        "./assets/HighChart/exporting.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }

  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [{
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
